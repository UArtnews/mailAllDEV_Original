{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*",
		"way/generators": "1.1",
        "makzumi/calendar": "dev-master",
        "tijsverkoyen/css-to-inline-styles": "1.3.0",
		"beelab/bowerphp": "0.1.*@alpha",
        "tijsverkoyen/css-to-inline-styles": "1.5.0",
        "maatwebsite/excel": "1.2.2"
	},
    "require-dev": {
        "fzaninotto/faker": "dev-master",
        "zizaco/testcases-laravel": "dev-master",
        "behat/behat": "3.0.*",
        "behat/mink": "1.5.*",
        "behat/mink-extension": "2.*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium-driver": "*",
        "behat/mink-selenium2-driver": "*"
    },
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/Libraries"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
